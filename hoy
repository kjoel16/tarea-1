using System.Collections.Generic;
using System.Text.Json.Serialization;

namespace TuProyecto.Alias.Dto
{
    public class AliasPaymentCredentialsResponseDto
    {
        [JsonPropertyName("id")]
        public string Id { get; set; }

        [JsonPropertyName("externalId")]
        public string ExternalId { get; set; }

        [JsonPropertyName("paymentCredentials")]
        public List<PaymentCredentialSummaryDto> PaymentCredentials { get; set; }
    }
}

using System.Text.Json.Serialization;

namespace TuProyecto.Alias.Dto
{
    public class PaymentCredentialSummaryDto
    {
        [JsonPropertyName("id")]
        public string Id { get; set; }

        [JsonPropertyName("type")]
        public string Type { get; set; }

        [JsonPropertyName("externalId")]
        public string ExternalId { get; set; }
    }
}
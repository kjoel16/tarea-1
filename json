package com.mycompany.api.transaccions.dto;

public class TransaccionResultDTO {

    private String codErr;
    private String descErr;

    public TransaccionResultDTO(String codErr, String descErr) {
        this.codErr = codErr;
        this.descErr = descErr;
    }

    public String getCodErr() { return codErr; }
    public String getDescErr() { return descErr; }
}